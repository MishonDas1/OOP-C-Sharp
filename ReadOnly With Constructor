using System;

namespace Myapp
{
    public class Person{
        public readonly int age;

        public Person(int age){
            this.age = age;
        }
    }
    class Program
    {
        static void Main(string[] args)
        {
            Person p1 = new Person(30);
            
            Console.WriteLine(p1.age);
        }
    }
}
